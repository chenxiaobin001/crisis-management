<!DOCTYPE html>
<html>
<head>
  <title>Login</title>
  <%= stylesheet_link_tag :all %>
  <!-- <%= javascript_include_tag :defaults %> -->
  <%= csrf_meta_tag %>
</head>
<body>
  <% flash.each do |name, msg| %>
    <%= content_tag :div, msg, :id => "flash#{name}" %>
  <% end %>
  
  
  <div id="user_nav">
  <% if current_user %>
    Logged in as <%= current_user.pennkey %>
    <%= link_to "Log out", log_out_path %>
  <% end %>
  </div>

<%= yield %>



</body>
</html>